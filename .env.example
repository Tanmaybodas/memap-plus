# GitHub (optional personal access token for higher rate limits)
GITHUB_TOKEN=

# Reddit API (required for Reddit data)
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
REDDIT_USER_AGENT=MeMapPlus/0.1 by YOUR_REDDIT_USERNAME

# Google Custom Search (required for web mentions)
GOOGLE_CSE_ID=
GOOGLE_API_KEY=

# Instagram login (optional, improves reliability)
IG_USERNAME=
IG_PASSWORD=

# Feature toggles (recommended fast demo: embeddings disabled)
ENABLE_EMBEDDINGS=false
ENABLE_IMAGE_SIMILARITY=true
